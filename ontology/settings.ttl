  autoins:isRequired "false"^^xsd:boolean ;
  autoins:formHelpText "Current health status" ;
  autoins:enumerationValues ("Healthy" "Warning" "Error" "Unknown" "Maintenance") ;
  monitoring:readOnly "true"^^xsd:boolean ;
