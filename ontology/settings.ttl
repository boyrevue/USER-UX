# INSURANCE PAYMENTS PROPERTIES
banking:paymentMethod a owl:DatatypeProperty ;
  rdfs:domain settings:InsurancePayments ;
  rdfs:range xsd:string ;
  rdfs:label "payment method" ;
  rdfs:label "Zahlungsmethode"@de ;
  autoins:isRequired "true"^^xsd:boolean ;
  autoins:formHelpText "Primary payment method for insurance premiums" ;
  autoins:formHelpText "Primäre Zahlungsmethode für Versicherungsprämien"@de ;
  autoins:enumerationValues ("Direct Debit" "Credit Card" "Debit Card" "Bank Transfer" "PayPal" "Apple Pay" "Google Pay") ;
  security:encryptionRequired "false"^^xsd:boolean .

banking:paymentFrequency a owl:DatatypeProperty ;
  rdfs:domain settings:InsurancePayments ;
  rdfs:range xsd:string ;
  rdfs:label "payment frequency" ;
  rdfs:label "Zahlungshäufigkeit"@de ;
  autoins:isRequired "true"^^xsd:boolean ;
  autoins:formHelpText "How often insurance payments are made" ;
  autoins:formHelpText "Wie oft Versicherungszahlungen geleistet werden"@de ;
  autoins:enumerationValues ("Monthly" "Quarterly" "Semi-Annually" "Annually") ;
  autoins:defaultValue "Monthly" ;
  security:encryptionRequired "false"^^xsd:boolean .

banking:premiumAmount a owl:DatatypeProperty ;
  rdfs:domain settings:InsurancePayments ;
  rdfs:range xsd:decimal ;
  rdfs:label "premium amount" ;
  rdfs:label "Prämienbetrag"@de ;
  autoins:isRequired "true"^^xsd:boolean ;
  autoins:formHelpText "Insurance premium amount in GBP" ;
  autoins:formHelpText "Versicherungsprämienbetrag in GBP"@de ;
  autoins:min "0.01" ;
  autoins:max "50000.00" ;
  autoins:validationPattern "^[0-9]+\\.[0-9]{2}$" ;
  autoins:errorMessage "Premium amount must be a valid currency amount (e.g., 123.45)" ;
  autoins:errorMessage "Prämienbetrag muss ein gültiger Währungsbetrag sein (z.B. 123,45)"@de ;
  security:encryptionRequired "true"^^xsd:boolean .

banking:paymentStartDate a owl:DatatypeProperty ;
  rdfs:domain settings:InsurancePayments ;
  rdfs:range xsd:date ;
  rdfs:label "payment start date" ;
  rdfs:label "Zahlungsbeginn"@de ;
  autoins:isRequired "true"^^xsd:boolean ;
  autoins:formHelpText "Date when insurance payments begin" ;
  autoins:formHelpText "Datum, an dem die Versicherungszahlungen beginnen"@de ;
  monitoring:alertDaysBefore "7" ;
  security:encryptionRequired "false"^^xsd:boolean .

banking:autoRenewal a owl:DatatypeProperty ;
  rdfs:domain settings:InsurancePayments ;
  rdfs:range xsd:boolean ;
  rdfs:label "auto renewal" ;
  rdfs:label "Automatische Verlängerung"@de ;
  autoins:isRequired "true"^^xsd:boolean ;
  autoins:formHelpText "Whether to automatically renew the insurance policy" ;
  autoins:formHelpText "Ob die Versicherungspolice automatisch verlängert werden soll"@de ;
  autoins:defaultValue "true"^^xsd:boolean ;
  security:encryptionRequired "false"^^xsd:boolean .

banking:discountCode a owl:DatatypeProperty ;
  rdfs:domain settings:InsurancePayments ;
  rdfs:range xsd:string ;
  rdfs:label "discount code" ;
  rdfs:label "Rabattcode"@de ;
  autoins:isRequired "false"^^xsd:boolean ;
  autoins:formHelpText "Optional discount or promotional code" ;
