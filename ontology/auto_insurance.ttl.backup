@prefix autoins: <https://autoins.example/ontology#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix schema: <https://schema.org/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix sh:     <http://www.w3.org/ns/shacl#> .

############################################################
## ONTOLOGY METADATA
############################################################

autoins: a owl:Ontology ;
  rdfs:label "AUTOINS Complete Car Insurance Ontology" ;
  owl:versionInfo "v2.0.0-2025-01-24" ;
  dct:creator "AUTOINS" ;
  dct:description """Complete production-ready ontology for motor insurance covering:
    1. Driver Management (up to 4 drivers with relationships)
    2. Vehicle Details (including comprehensive modifications)
    3. Policy Configuration (coverage, exclusions, excesses)
    4. Claims History (5-year lookback, max 5 claims)
    5. Payment & Start Date
    6. Optional Extras
    7. Marketing Preferences
    8. Validation Rules & SHACL Shapes
    
    Includes complete enumerations, helper texts, form guidance, and validation support.""" ;
  dct:rights "Copyright 2025 AUTOINS" ;
  dct:license <https://creativecommons.org/licenses/by/4.0/> .

############################################################
## ANNOTATION PROPERTIES & HELPERS
############################################################

# Field Collection & Requirements
autoins:collectionPhase a owl:AnnotationProperty ;
  rdfs:label "collection phase" ;
  rdfs:range skos:Concept ;
  rdfs:comment "Marks whether field is user-provided, insurer-provided, or system-derived" .

autoins:requiredForQuote a owl:AnnotationProperty ;
  rdfs:label "required for quote" ;
  rdfs:range xsd:boolean ;
  rdfs:comment "Whether this attribute is required to compute a price" .

autoins:isRequired a owl:AnnotationProperty ;
  rdfs:label "is required" ;
  rdfs:range xsd:boolean ;
  rdfs:comment "Whether field is mandatory for form completion" .

autoins:typicalDocSource a owl:AnnotationProperty ;
  rdfs:label "typical document source" ;
  rdfs:range autoins:InsuranceDocument ;
  rdfs:comment "Where this attribute is usually found in documents" .

autoins:validationPattern a owl:AnnotationProperty ;
  rdfs:label "validation pattern" ;
  rdfs:range xsd:string ;
  rdfs:comment "Regex pattern for field validation" .

autoins:formHelpText a owl:AnnotationProperty ;
  rdfs:label "form help text" ;
  rdfs:range xsd:string ;
  rdfs:comment "Helper text to display in forms" .

autoins:errorMessage a owl:AnnotationProperty ;
  rdfs:label "error message" ;
  rdfs:range xsd:string ;
  rdfs:comment "Error message for validation failures" .

autoins:conditionalRequirement a owl:AnnotationProperty ;
  rdfs:label "conditional requirement" ;
  rdfs:range xsd:string ;
  rdfs:comment "Describes when field becomes required" .

# Collection Phase Scheme
autoins:CollectionPhaseScheme a skos:ConceptScheme ;
  skos:prefLabel "Field Collection Phase Scheme" .

autoins:OnboardingField a skos:Concept ;
  skos:inScheme autoins:CollectionPhaseScheme ;
  skos:prefLabel "User-Provided (Onboarding)" .

autoins:InsurerProvidedField a skos:Concept ;
  skos:inScheme autoins:CollectionPhaseScheme ;
  skos:prefLabel "Insurer-Provided (Quote/Policy)" .

autoins:SystemDerivedField a skos:Concept ;
  skos:inScheme autoins:CollectionPhaseScheme ;
  skos:prefLabel "System-Derived (Lookups/Calculations)" .

############################################################
## HELPER CLASSES FOR FORMS & UI
############################################################

autoins:FormHelper a owl:Class ;
  rdfs:label "Form Helper" ;
  rdfs:comment "Base class for form assistance elements" .

autoins:ValidationRule a owl:Class ;
  rdfs:label "Validation Rule" ;
  rdfs:comment "Validation rules for form fields" .

autoins:ConditionalField a owl:Class ;
  rdfs:label "Conditional Field" ;
  rdfs:comment "Fields that appear based on conditions" .

autoins:ProgressIndicator a owl:Class ;
  rdfs:label "Progress Indicator" ;
  rdfs:comment "Form completion progress tracking" .

# Form Section Definitions
autoins:FormSection a owl:Class ;
  rdfs:label "Form Section" ;
  rdfs:comment "Logical grouping of form fields" .

autoins:SECTION_DRIVERS a autoins:FormSection ;
  rdfs:label "Driver Details Section" ;
  autoins:sectionOrder 1 ;
  autoins:sectionDescription "Add up to 4 drivers including yourself and their convictions" .

autoins:SECTION_VEHICLE a autoins:FormSection ;
  rdfs:label "Vehicle Details Section" ;
  autoins:sectionOrder 2 ;
  autoins:sectionDescription "Your vehicle information and modifications" .

autoins:SECTION_POLICY a autoins:FormSection ;
  rdfs:label "Policy Configuration Section" ;
  autoins:sectionOrder 3 ;
  autoins:sectionDescription "Coverage type and policy options" .

autoins:SECTION_CLAIMS a autoins:FormSection ;
  rdfs:label "Claims History Section" ;
  autoins:sectionOrder 4 ;
  autoins:sectionDescription "Insurance claims in last 5 years" .

autoins:SECTION_PAYMENT a autoins:FormSection ;
  rdfs:label "Payment Section" ;
  autoins:sectionOrder 5 ;
  autoins:sectionDescription "Payment method and start date" .

autoins:SECTION_EXTRAS a autoins:FormSection ;
  rdfs:label "Optional Extras Section" ;
  autoins:sectionOrder 6 ;
  autoins:sectionDescription "Additional coverage options" .

autoins:SECTION_MARKETING a autoins:FormSection ;
  rdfs:label "Marketing Preferences Section" ;
  autoins:sectionOrder 7 ;
  autoins:sectionDescription "Communication preferences" .

############################################################
## COMPREHENSIVE INSURANCE DOMAIN ONTOLOGY
############################################################

# ========================================
# ROOT INSURANCE CLASSES
# ========================================

# Root Insurance Entity
autoins:InsuranceEntity a owl:Class ;
    rdfs:label "Insurance Entity" ;
    rdfs:comment "The root class for all insurance-related entities, documents, and concepts" ;
    rdfs:subClassOf owl:Thing .

# ========================================
# PRIMARY INSURANCE CATEGORIES
# ========================================

# Motor Insurance Documents
autoins:MotorInsuranceDocument a owl:Class ;
    rdfs:label "Motor Insurance Document" ;
    rdfs:comment "Documents related to motor vehicle insurance" ;
    rdfs:subClassOf autoins:InsuranceEntity .

# Driver Documents
autoins:DriverDocument a owl:Class ;
    rdfs:label "Driver Document" ;
    rdfs:comment "Documents related to driver qualifications and history" ;
    rdfs:subClassOf autoins:InsuranceEntity .

# Vehicle Documents
autoins:VehicleDocument a owl:Class ;
    rdfs:label "Vehicle Document" ;
    rdfs:comment "Documents related to vehicle specifications and ownership" ;
    rdfs:subClassOf autoins:InsuranceEntity .

# Claims Documents
autoins:ClaimsDocument a owl:Class ;
    rdfs:label "Claims Document" ;
    rdfs:comment "Documents related to insurance claims and incidents" ;
    rdfs:subClassOf autoins:InsuranceEntity .

# Policy Documents
autoins:PolicyDocument a owl:Class ;
    rdfs:label "Policy Document" ;
    rdfs:comment "Insurance policy contracts and related documentation" ;
    rdfs:subClassOf autoins:InsuranceEntity .

# Financial Documents
autoins:FinancialDocument a owl:Class ;
    rdfs:label "Financial Document" ;
    rdfs:comment "Payment, premium, and financial transaction documents" ;
    rdfs:subClassOf autoins:InsuranceEntity .

