############################################################
## SHACL VALIDATION SHAPES FOR BUSINESS RULES
############################################################

# SHACL shape for conditional visibility of relationship field
autoins:RelationshipVisibilityShape a sh:NodeShape ;
  sh:targetClass autoins:Driver ;
  sh:property [
    sh:path autoins:relationshipToMainDriver ;
    sh:condition [
      sh:property [
        sh:path autoins:driverClassification ;
        sh:hasValue "MAIN" ;
      ] ;
    ] ;
    sh:maxCount 0 ;
    sh:message "Relationship to main driver should not be specified for the main driver" ;
  ] ;
  sh:property [
    sh:path autoins:relationshipToMainDriver ;
    sh:condition [
      sh:property [
        sh:path autoins:driverClassification ;
        sh:hasValue "NAMED" ;
      ] ;
    ] ;
    sh:minCount 1 ;
    sh:message "Relationship to main driver is required for named drivers" ;
  ] .

# SHACL shape for living at same address field
autoins:LivingAddressVisibilityShape a sh:NodeShape ;
  sh:targetClass autoins:Driver ;
  sh:property [
    sh:path autoins:livingAtSameAddress ;
    sh:condition [
      sh:property [
        sh:path autoins:driverClassification ;
        sh:hasValue "MAIN" ;
      ] ;
    ] ;
    sh:maxCount 0 ;
    sh:message "Living at same address should not be specified for the main driver" ;
  ] ;
  sh:property [
    sh:path autoins:livingAtSameAddress ;
    sh:condition [
      sh:property [
        sh:path autoins:driverClassification ;
        sh:hasValue "NAMED" ;
      ] ;
    ] ;
    sh:minCount 1 ;
    sh:message "Living at same address is required for named drivers" ;
  ] .

# Main Driver Constraint - Only one driver can be the main driver
autoins:MainDriverConstraint a sh:NodeShape ;
  sh:targetClass autoins:InsurancePolicy ;
  sh:property [
    sh:path autoins:hasDriver ;
    sh:qualifiedValueShape [
      sh:path autoins:driverClassification ;
      sh:hasValue "MAIN" ;
    ] ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedMaxCount 1 ;
    sh:message "Exactly one driver must be designated as the main driver" ;
  ] .

# Driver Classification Validation
autoins:DriverClassificationShape a sh:NodeShape ;
  sh:targetClass autoins:Driver ;
  sh:property [
    sh:path autoins:driverClassification ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:in ("MAIN" "NAMED") ;
    sh:message "Driver classification must be either MAIN or NAMED" ;
  ] .

# Named Driver Requirements - If not main driver, relationship is required
autoins:NamedDriverRequirements a sh:NodeShape ;
  sh:targetClass autoins:Driver ;
  sh:property [
    sh:path autoins:relationshipToMainDriver ;
    sh:minCount 1 ;
    sh:message "Relationship to main driver is required for named drivers" ;
  ] ;
  sh:not [
    sh:property [
      sh:path autoins:driverClassification ;
      sh:hasValue "MAIN" ;
    ] ;
  ] .

# Main Driver Address Requirements - Main driver must have complete address
autoins:MainDriverAddressShape a sh:NodeShape ;
  sh:targetClass autoins:Driver ;
  sh:property [
    sh:path autoins:addressLine1 ;
    sh:minCount 1 ;
    sh:message "Main driver must provide complete address" ;
  ] ;
  sh:property [
    sh:path autoins:city ;
    sh:minCount 1 ;
    sh:message "Main driver must provide city" ;
  ] ;
  sh:property [
    sh:path autoins:postcode ;
    sh:minCount 1 ;
    sh:message "Main driver must provide postcode" ;
  ] ;
  sh:property [
    sh:path autoins:email ;
    sh:minCount 1 ;
    sh:message "Main driver must provide email address" ;
  ] ;
  sh:property [
    sh:path autoins:mobilePhone ;
    sh:minCount 1 ;
    sh:message "Main driver must provide mobile phone" ;
  ] ;
  sh:and [
    sh:property [
      sh:path autoins:driverClassification ;
      sh:hasValue "MAIN" ;
    ] ;
  ] .

