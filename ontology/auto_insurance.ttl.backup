  ] .

############################################################
## SHACL VALIDATION SHAPES
############################################################

# Driver Validation Shape
autoins:DriverShape a sh:NodeShape ;
  sh:targetClass autoins:Driver ;
  sh:property [
    sh:path autoins:firstName ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string ;
    sh:minLength 1 ;
    sh:maxLength 50 ;
    sh:pattern "^[A-Za-z\\s\\-']+$" ;
    sh:message "First name required (letters only, max 50 chars)"
  ] ;
  sh:property [
    sh:path autoins:dateOfBirth ;
    sh:minCount 1 ;
    sh:datatype xsd:date ;
    sh:message "Date of birth required"
  ] ;
  sh:sparql [
    sh:message "Driver must be between 17 and 99 years old" ;
    sh:select """
      SELECT $this WHERE {
        $this autoins:dateOfBirth ?dob .
        BIND(YEAR(NOW()) - YEAR(?dob) AS ?age)
        FILTER(?age < 17 || ?age > 99)
      }"""
  ] .

# Vehicle Validation Shape
autoins:VehicleShape a sh:NodeShape ;
  sh:targetClass autoins:Vehicle ;
  sh:property [
    sh:path autoins:registrationNumber ;
    sh:minCount 1 ;
    sh:pattern "^[A-Z]{2}[0-9]{2}\\s?[A-Z]{3}$|^[A-Z][0-9]{1,3}\\s?[A-Z]{3}$" ;
    sh:message "Valid UK registration required"
  ] ;
  sh:property [
    sh:path autoins:annualMileage ;
    sh:minCount 1 ;
    sh:minInclusive 0 ;
    sh:maxInclusive 50000 ;
    sh:message "Annual mileage required (0-50000)"
  ] .

# Claims Validation Shape
autoins:ClaimShape a sh:NodeShape ;
  sh:targetClass autoins:Claim ;
  sh:sparql [
    sh:message "Claim must be within last 5 years" ;
    sh:select """
      SELECT $this WHERE {
        $this autoins:claimDate ?date .
        BIND(YEAR(NOW()) - YEAR(?date) AS ?years)
        FILTER(?years > 5)
      }"""
  ] ;
  sh:sparql [
    sh:message "Crime reference required if police reported" ;
    sh:select """
      SELECT $this WHERE {
        $this autoins:policeReported true .
        FILTER NOT EXISTS { $this autoins:crimeReference ?ref }
      }"""
  ] .

# Policy Validation Shape
autoins:PolicyShape a sh:NodeShape ;
  sh:targetClass autoins:InsurancePolicy ;
  sh:property [
    sh:path autoins:coverType ;
    sh:minCount 1 ;
    sh:in ("COMPREHENSIVE" "THIRD_PARTY_FIRE_THEFT" "THIRD_PARTY") ;
    sh:message "Valid coverage type required"
  ] .

