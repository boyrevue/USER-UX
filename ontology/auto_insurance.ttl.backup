# ========================================
# COMPREHENSIVE DOCUMENT PROPERTIES
# ========================================

# Document Identification Properties
autoins:documentID a owl:DatatypeProperty ;
    rdfs:label "document ID" ;
    rdfs:comment "Unique identifier for the insurance document" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:string ;
    autoins:isRequired "true"^^xsd:boolean ;
    autoins:formHelpText "Enter the unique document reference number" .

autoins:documentNumber a owl:DatatypeProperty ;
    rdfs:label "document number" ;
    rdfs:comment "Official document number or reference" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:string ;
    autoins:formHelpText "Official document number as shown on the document" .

autoins:documentTitle a owl:DatatypeProperty ;
    rdfs:label "document title" ;
    rdfs:comment "Official title of the insurance document" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:string .

autoins:documentType a owl:DatatypeProperty ;
    rdfs:label "document type" ;
    rdfs:comment "Category or classification of the document" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:string ;
    autoins:enumerationValues ("Certificate" "Schedule" "Quote" "Policy" "Claim" "Receipt" "Notice") .

autoins:documentStatus a owl:DatatypeProperty ;
    rdfs:label "document status" ;
    rdfs:comment "Current status of the document" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:string ;
    autoins:enumerationValues ("Active" "Expired" "Cancelled" "Suspended" "Draft" "Pending") .

# Temporal Properties for Documents
autoins:documentIssueDate a owl:DatatypeProperty ;
    rdfs:label "document issue date" ;
    rdfs:comment "Date when the document was issued" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:date ;
    autoins:formHelpText "Date the document was created or issued" .

autoins:documentExpiryDate a owl:DatatypeProperty ;
    rdfs:label "document expiry date" ;
    rdfs:comment "Date when the document expires" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:date ;
    autoins:formHelpText "Date when this document becomes invalid" .

autoins:documentValidFrom a owl:DatatypeProperty ;
    rdfs:label "document valid from" ;
    rdfs:comment "Date from which the document is valid" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:date .

autoins:documentValidUntil a owl:DatatypeProperty ;
    rdfs:label "document valid until" ;
    rdfs:comment "Date until which the document remains valid" ;
    rdfs:domain autoins:InsuranceEntity ;
    rdfs:range xsd:date .

# Policy-Specific Properties
autoins:policyNumber a owl:DatatypeProperty ;
    rdfs:label "policy number" ;
    rdfs:comment "Unique policy identifier" ;
    rdfs:domain autoins:PolicyDocument ;
    rdfs:range xsd:string ;
    autoins:isRequired "true"^^xsd:boolean ;
    autoins:formHelpText "Your insurance policy number" ;
    autoins:validationPattern "^[A-Z]{2,4}[0-9]{6,12}$" .

autoins:certificateNumber a owl:DatatypeProperty ;
    rdfs:label "certificate number" ;
    rdfs:comment "Insurance certificate reference number" ;
    rdfs:domain autoins:InsuranceCertificate ;
    rdfs:range xsd:string ;
    autoins:formHelpText "Certificate number from your insurance certificate" .

autoins:quoteReference a owl:DatatypeProperty ;
    rdfs:label "quote reference" ;
    rdfs:comment "Insurance quote reference number" ;
    rdfs:domain autoins:QuoteDocument ;
    rdfs:range xsd:string ;
    autoins:formHelpText "Quote reference provided by the insurer" .

# Claims-Specific Properties
autoins:claimNumber a owl:DatatypeProperty ;
    rdfs:label "claim number" ;
    rdfs:comment "Unique claim reference number" ;
    rdfs:domain autoins:ClaimsDocument ;
    rdfs:range xsd:string ;
    autoins:isRequired "true"^^xsd:boolean ;
    autoins:formHelpText "Claim reference number provided by your insurer" .

autoins:incidentDate a owl:DatatypeProperty ;
    rdfs:label "incident date" ;
    rdfs:comment "Date when the incident occurred" ;
    rdfs:domain autoins:ClaimsDocument ;
    rdfs:range xsd:date ;
    autoins:isRequired "true"^^xsd:boolean ;
    autoins:formHelpText "Date of the accident or incident" .

autoins:reportedDate a owl:DatatypeProperty ;
    rdfs:label "reported date" ;
    rdfs:comment "Date when the incident was reported to insurer" ;
    rdfs:domain autoins:ClaimsDocument ;
    rdfs:range xsd:date ;
    autoins:formHelpText "Date you notified your insurer of the incident" .

autoins:claimAmount a owl:DatatypeProperty ;
    rdfs:label "claim amount" ;
    rdfs:comment "Total amount claimed" ;
    rdfs:domain autoins:ClaimsDocument ;
    rdfs:range xsd:decimal ;
    autoins:formHelpText "Total amount being claimed (£)" .

autoins:settledAmount a owl:DatatypeProperty ;
    rdfs:label "settled amount" ;
    rdfs:comment "Amount paid by insurer" ;
    rdfs:domain autoins:ClaimsDocument ;
    rdfs:range xsd:decimal ;
    autoins:formHelpText "Amount actually paid by the insurer (£)" .

# Financial Properties
autoins:premiumAmount a owl:DatatypeProperty ;
    rdfs:label "premium amount" ;
    rdfs:comment "Insurance premium amount" ;
    rdfs:domain autoins:FinancialDocument ;
    rdfs:range xsd:decimal ;
    autoins:formHelpText "Premium amount in pounds (£)" .

autoins:currency a owl:DatatypeProperty ;
    rdfs:label "currency" ;
    rdfs:comment "Currency code (ISO 4217)" ;
    rdfs:domain autoins:FinancialDocument ;
    rdfs:range xsd:string ;
    autoins:enumerationValues ("GBP" "EUR" "USD") .

autoins:paymentMethod a owl:DatatypeProperty ;
    rdfs:label "payment method" ;
    rdfs:comment "Method of premium payment" ;
    rdfs:domain autoins:FinancialDocument ;
    rdfs:range xsd:string ;
    autoins:enumerationValues ("Direct Debit" "Credit Card" "Debit Card" "Bank Transfer" "Cheque" "Cash") .

autoins:paymentFrequency a owl:DatatypeProperty ;
    rdfs:label "payment frequency" ;
    rdfs:comment "How often premiums are paid" ;
    rdfs:domain autoins:FinancialDocument ;
    rdfs:range xsd:string ;
    autoins:enumerationValues ("Annual" "Monthly" "Quarterly" "One-off") .

