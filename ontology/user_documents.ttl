############################################################
## SHACL VALIDATION SHAPES
############################################################

docs:PassportDocumentShape a sh:NodeShape ;
  sh:targetClass docs:PassportDocument ;
  sh:property [
    sh:path docs:passportNumber ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
    sh:message "Passport number is required"@en ;
  ] ;
  sh:property [
    sh:path docs:givenNames ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
    sh:message "Given names are required"@en ;
  ] ;
  sh:property [
    sh:path docs:surname ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
    sh:message "Surname is required"@en ;
  ] ;
  sh:property [
    sh:path docs:dateOfBirth ;
    sh:minCount 1 ;
    sh:datatype xsd:date ;
    sh:message "Date of birth is required"@en ;
  ] ;
  sh:property [
    sh:path docs:dateOfExpiry ;
    sh:minCount 1 ;
    sh:datatype xsd:date ;
    sh:message "Expiry date is required"@en ;
  ] ;
  sh:property [
    sh:path docs:nationality ;
    sh:minCount 1 ;
    sh:in ("GBR" "USA" "DEU" "FRA" "ESP" "ITA" "NLD" "BEL" "IRL" "Other") ;
    sh:message "Valid nationality code is required"@en ;
  ] .

docs:DrivingLicenceDocumentShape a sh:NodeShape ;
  sh:targetClass docs:DrivingLicenceDocument ;
  sh:property [
    sh:path docs:licenceNumber ;
    sh:minCount 1 ;
    sh:pattern "^[A-Z]{5}[0-9]{6}[A-Z]{2}[0-9]{2}$" ;
    sh:message "Valid UK driving licence number is required"@en ;
  ] ;
  sh:property [
    sh:path docs:licenceIssueDate ;
    sh:minCount 1 ;
    sh:datatype xsd:date ;
    sh:message "Licence issue date is required"@en ;
  ] ;
  sh:property [
    sh:path docs:licenceExpiryDate ;
    sh:minCount 1 ;
    sh:datatype xsd:date ;
    sh:message "Licence expiry date is required"@en ;
  ] ;
  sh:property [
    sh:path docs:licenceCategories ;
    sh:minCount 1 ;
    sh:message "At least one licence category is required"@en ;
  ] .

docs:DocumentProcessingShape a sh:NodeShape ;
  sh:targetClass docs:PersonalDocument ;
  sh:property [
    sh:path docs:ocrSupported ;
    sh:datatype xsd:boolean ;
    sh:message "OCR support flag must be boolean"@en ;
  ] ;
  sh:property [
    sh:path docs:confidenceThreshold ;
    sh:datatype xsd:decimal ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Confidence threshold must be between 0.0 and 1.0"@en ;
  ] .
