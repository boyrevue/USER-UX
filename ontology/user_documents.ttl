############################################################
## USER DOCUMENTS ONTOLOGY - COMPREHENSIVE DOCUMENT MANAGEMENT
############################################################

docs: a owl:Ontology ;
  rdfs:label "CLIENT-UX User Documents Ontology" ;
  owl:versionInfo "v1.0.0-2025-01-28" ;
  dct:creator "CLIENT-UX" ;
  dct:description """Complete ontology for personal document management covering:
    1. Passport Documents (MRZ extraction, OCR processing)
    2. Driving Licence Documents (UK DVLA format)
    3. Identity Cards (National ID, EU ID cards)
    4. Utility Bills (Proof of address)
    5. Bank Statements (Financial verification)
    6. Medical Certificates (Health documentation)
    7. Vehicle Registration (V5C, ownership documents)
    8. Insurance Documents (Policies, quotes, claims)
    
    Includes OCR field extraction, validation rules, and multi-language support.""" ;
  dct:rights "Copyright 2025 CLIENT-UX" ;
  dct:license <https://creativecommons.org/licenses/by/4.0/> .

############################################################
## DOCUMENT BASE CLASSES
############################################################

docs:PersonalDocument a owl:Class ;
  rdfs:label "Personal Document"@en ;
  rdfs:label "Persönliches Dokument"@de ;
  rdfs:comment "Base class for all personal documents that can be processed by CLIENT-UX" .

docs:IdentityDocument a owl:Class ;
  rdfs:subClassOf docs:PersonalDocument ;
  rdfs:label "Identity Document"@en ;
  rdfs:label "Ausweisdokument"@de ;
  rdfs:comment "Documents that establish identity (passport, driving licence, ID card)" .

docs:ProofOfAddress a owl:Class ;
  rdfs:subClassOf docs:PersonalDocument ;
  rdfs:label "Proof of Address"@en ;
  rdfs:label "Adressnachweis"@de ;
  rdfs:comment "Documents that prove residential address" .

docs:FinancialDocument a owl:Class ;
  rdfs:subClassOf docs:PersonalDocument ;
  rdfs:label "Financial Document"@en ;
  rdfs:label "Finanzdokument"@de ;
  rdfs:comment "Financial statements, bank documents, insurance policies" .

############################################################
## PASSPORT DOCUMENT CLASS
############################################################

docs:PassportDocument a owl:Class ;
  rdfs:subClassOf docs:IdentityDocument ;
  rdfs:label "Passport Document"@en ;
  rdfs:label "Reisedokument"@de ;
  rdfs:comment "Passport document with MRZ (Machine Readable Zone) extraction capabilities" ;
  docs:ocrSupported "true"^^xsd:boolean ;
  docs:mrzSupported "true"^^xsd:boolean ;
  docs:confidenceThreshold "0.85"^^xsd:decimal .

############################################################
## DRIVING LICENCE DOCUMENT CLASS
############################################################

docs:DrivingLicenceDocument a owl:Class ;
  rdfs:subClassOf docs:IdentityDocument ;
  rdfs:label "Driving Licence Document"@en ;
  rdfs:label "Führerscheindokument"@de ;
  rdfs:comment "UK DVLA driving licence with photo and entitlement extraction" ;
  docs:ocrSupported "true"^^xsd:boolean ;
  docs:dvlaCompliant "true"^^xsd:boolean ;
  docs:confidenceThreshold "0.80"^^xsd:decimal .

